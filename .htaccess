	RewriteCond %{REQUEST_URI} ^/media/css/.*\.css$ [OR]
	RewriteCond %{REQUEST_URI} ^/media/js/.*\.js$	
	RewriteRule (.*)\.(\d{10})\.(gif|png|jpg)$ $1.$3 [L,NC]
	RewriteRule (.*)\.(\d{10})\.(css)$ $1.$3 [L,NC]
	RewriteRule (.*)\.(\d{10})\.(js)$ $1.$3 [L,NC]	
    RewriteCond %{HTTPS} !=on 
